<%= form_for @company, :html => {:multipart => true} do |f| %>

<p><%= f.label :name %></p>
<p><%= f.text_field :name %></p>

<p><%=f.label :webstie %></p>
<p><%= f.text_field :website %></p>

<p><%= f.label :notes %></p>
<p><%= f.text_area :notes %></p>

<p><%= f.label :source %></p>
<p><%= f.text_field :source %></p>

<%= f.submit %>

<% end %>